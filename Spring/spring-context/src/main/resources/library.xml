<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">
            
            <bean id="lib" class="org.jsp.Collection_injection.Library">
				<property name="books">
					<list>
						<value>JAVA</value>
						<value>Java Script</value>
						<value>React-JS</value>						
					</list>
				</property>
				<property name="author">
					<set>
						<value>James ghosling</value>
						<value>Ananymous</value>
						<value>james Rod</value>
					</set>
				</property>
				<property name="details">
					<map>
						<entry key="JAVA" value="James ghosling"></entry>
						<entry key="Java Script" value="Ananymous"></entry>
						<entry key="React-JS" value="james Rod"></entry>
					</map>
				</property>
			</bean>
            
            
            
</beans>